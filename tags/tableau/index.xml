    lon &lt;span style=&#34;color:#f92672&#34;&gt;=&lt;/span&gt; place[&lt;span style=&#34;color:#e6db74&#34;&gt;&amp;#39;coordinates&amp;#39;&lt;/span&gt;][&lt;span style=&#34;color:#ae81ff&#34;&gt;1&lt;/span&gt;]
